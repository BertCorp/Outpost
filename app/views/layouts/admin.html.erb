<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Outpost Admin Panel - <%= content_for?(:title) ? yield(:title) : "Automated User Testing Made Easy" %></title>
    <!--<meta name="description" content="">
    <meta name="author" content="">
    <link rel="shortcut icon" href="/favicon.ico">
    <link rel="apple-touch-icon" href="/apple-touch-icon.png">
    <link rel="apple-touch-icon" sizes="72x72" href="/apple-touch-icon-ipad.png" />
    <link rel="apple-touch-icon" sizes="114x114" href="/apple-touch-icon-iphone4.png" />-->
		<!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
		<!--[if lt IE 9]>
		<%= javascript_include_tag "bootstrap-ie" %>
		<![endif]-->
    <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
    <%= csrf_meta_tags %>
    <%= javascript_include_tag "//cdnjs.cloudflare.com/ajax/libs/jquery/2.0.3/jquery.min.js" %>
		<!--<%= javascript_include_tag "/assets/jquery-2.0.3.min.js" %>-->
    <%= yield(:head) %>
  </head>
  <body>
    <div class="container">
			<%= render 'layouts/header' %>      
			
			<div class="content">
          <%= render 'layouts/messages' %>
					
					<%= render 'layouts/admin_nav' %>

          <%= yield %>
      </div>

			<%= render 'layouts/footer' %>
		</div>
  	<%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  	<%= render 'layouts/analytics' %>
	</body>
</html>
