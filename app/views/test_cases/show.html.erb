<p id="notice"><%= notice %></p>
<%= link_to 'Edit', edit_test_case_path(@test_case) %> |
<%= link_to 'Back', test_cases_path %>

  <p><b>Title: <%= @test_case.title %></b></p>
  
  <p>Description: <%= @test_case.description %></p>

  <p>Company ID: <%= @test_case.company_id %></p>

  <p>Test Suite ID: <%= @test_case.test_suite_id %></p>
  
  <p>Status: <%= @test_case.status %></p>

  <p>Setup Started At: <%= @test_case.setup_started_at %></p>

  <p>Setup Completed At:<%= @test_case.setup_completed_at %></p>

  <p>Pending Message: <%= @test_case.pending_message %></p>
  
<table class="table table-bordered table-striped">
  <% @test_case.results.each do |results| %>
  <tr>
    <td><%= results.id %></td>
    <td><%= results.status %></td>
    <td><%= results.report_id %></td>
  </tr>
  <% end %>
</table>
